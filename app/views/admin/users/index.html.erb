<h1>Rotten Mangoes Users</h1>
    
    <%= paginate @users %>
    <% @users.each do |user| %>
      <h3>First Name: <%= user.firstname %></h3>
      <h3>Last Name: <%= user.lastname %></h3>
      <h3>Email: <%= user.email %></h3>
      <h3>Created On: <%= user.created_at %></h3>
      <p><%= link_to "User details", admin_user_path(user) %></p>
    <% end %>
